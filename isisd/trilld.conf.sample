! -*- trill -*-
!
! ISISd sample configuration file
!
hostname lab221-i7i7
password foo
!log file /var/log/trill.log
log syslog warnings
log stdout

router isis DEAD
        net 00.0050.bae3.3b8e.00
trill instance br0
trill nickname 10
trill nickname priority 10
interface eth0
        trill router trill DEAD
interface mpls0
	trillpe router trill DEAD
